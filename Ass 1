#include<stdio.h>
main()
{
	int a[100],n,i,max,min;
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	min=max=a[0];
	for(i=0;i<n;i++)
	{
		if(min>a[i])
		min=a[i];
		if(max<a[i])
		max=a[i];
	}
	printf("\nminimum array elements=%d\t",min);
	printf("\nmaximum array elements=%d\t",max);
	
}
